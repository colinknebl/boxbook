{
    "name": "boxbook",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:graphql": "cd graphql && npm run dev",
        "run:client": "next dev",
        "dev": "npm-run-all start:graphql run:client",
        "docker:stop": "docker stop $(docker ps -a -q)",
        "docker:rm": "docker rm $(docker ps -a -q)",
        "build": "next build",
        "start": "next start",
        "deploy:dev": "now --dev",
        "deploy:prod": "npm run test && now --prod",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.2",
        "@apollo/react-ssr": "^3.1.2",
        "@types/mongodb": "^3.3.3",
        "apollo-cache-inmemory": "^1.6.3",
        "apollo-client": "^2.6.4",
        "apollo-link-http": "^1.5.16",
        "apollo-link-schema": "^1.2.4",
        "apollo-server-micro": "^2.9.4",
        "mongodb": "^3.3.2",
        "next": "9.0.6",
        "node-fetch": "^2.6.0",
        "prisma-client-lib": "^1.34.8",
        "react": "16.9.0",
        "react-dom": "16.9.0"
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.5",
        "@types/styled-jsx": "^2.2.8",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "jest": "^24.9.0",
        "now": "^16.2.0",
        "npm-run-all": "^4.1.5",
        "typescript": "^3.6.3"
    }
}
